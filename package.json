{
  "name": "zepkit",
  "version": "1.0.0",
  "description": "ZeppelinOS kit containing React, ZeppelinOS & OpenZeppelin",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:tf": "truffle test ./test/*",
    "start-blank": "rm -rf client/src && cp -rf barebones/src client/",
    "client": "cd client && npm run start",
    "console": "truffle console --network development",
    "push": "zos push --deploy-dependencies",
    "session": "zos session --network development --expires 7200",
    "migrate": "truffle migrate --reset --network development",
    "migrate:ropsten": "truffle migrate --reset --network ropsten",
    "migrate:kovan": "truffle migrate --reset --network kovan"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeppelinos/zepkit"
  },
  "keywords": [
    "zeppelinos",
    "openzeppelin",
    "truffle",
    "react"
  ],
  "author": "Ramon Recuero <ramon@dapis.io>, Igor Yalovoy <to@ylv.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeppelinos/zepkit/issues"
  },
  "homepage": "https://github.com/zeppelinos/zepkit/blob/stable/README.md",
  "dependencies": {
    "dotenv": "^6.2.0",
    "openzeppelin-eth": "^2.1.3",
    "truffle-hdwallet-provider": "^1.0.3",
    "zos-lib": "2.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "openzeppelin-test-helpers": "^0.3.0",
    "zos": "^2.3.0"
  }
}
